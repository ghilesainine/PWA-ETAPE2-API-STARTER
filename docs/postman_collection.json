  },
  "item": [
    {
      "name": "Auth - Register",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "{{baseUrl}}/api/auth/register",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "api",
            "auth",
            "register"
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\"nom\": \"Reda\", \"email\": \"reda@example.com\", \"password\": \"secret123\"}"
        }
      }
    },
    {
      "name": "Auth - Login",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "{{baseUrl}}/api/auth/login",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "api",
            "auth",
            "login"
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\"email\": \"admin@example.com\", \"password\": \"admin123\"}"
        }
      }
    },
    {
      "name": "Users - Me",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{token}}"
          }
        ],
        "url": {
          "raw": "{{baseUrl}}/api/users/me",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "api",
            "users",
            "me"
          ]
        }
      }
    },
    {
      "name": "Ouvrages - List",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{baseUrl}}/api/ouvrages",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "api",
            "ouvrages"
          ]
        }
      }
    }
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:3000"
    },
    {
      "key": "token",
      "value": ""
    }
  ]
